{
    "name": "stepanenko3/nova-filemanager",
    "description": "File manager tool for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "filemanager"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^4.0",
        "pion/laravel-chunk-upload": "^1.5",
        "stepanenko3/laravel-helpers": "*",
        "stepanenko3/laravel-pagination": "*"
    },
    "autoload": {
        "psr-4": {
            "Stepanenko3\\NovaFileManager\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\NovaFileManager\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://laravelsatis.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "tightenco/duster": "^2.7"
    }
}
